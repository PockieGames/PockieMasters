{
  "name": "k8w-crypto",
  "version": "0.2.0",
  "description": "Crypto library (support both node and browser)",
  "main": "index.cjs",
  "exports": {
    "require": "./index.cjs",
    "import": "./index.mjs"
  },
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha",
    "build": "rm -rf dist && npx rollup -c && cp package.json LICENSE README.md dist/ && find dist/test -name \"*.d.ts\" | xargs rm -rf"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/k8w/k8w-crypto.git"
  },
  "keywords": [
    "k8w",
    "crypto"
  ],
  "author": "k8w",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.0.23",
    "mocha": "^8.3.0",
    "rollup": "^2.40.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "typings": "index.d.ts"
}
